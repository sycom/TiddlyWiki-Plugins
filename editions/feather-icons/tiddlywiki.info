{
	"description": "feather icons edition for plugin test",
	"plugins": [
			"tiddlywiki/tiddlyweb",
			"tiddlywiki/filesystem",
			"tiddlywiki/markdown",
      "sycom/g-analytics",
      "sycom/feather-icons"
	],
	"languages": [
		"fr-FR"
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"index": [
			"--rendertiddler","$:/plugins/tiddlywiki/tiddlyweb/save/offline","../../../files/feather-icons.html","text/plain"],
		"favicon": [
			"--savetiddler","$:/plugins/sycom/feather-icons/icon","favicon.ico"]
	},
	"config": {
		"retain-original-tiddler-path": true
	}
}
