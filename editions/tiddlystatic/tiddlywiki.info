{
	"description": "tiddlystatic edition for plugin test",
	"plugins": [
		"tiddlywiki/tiddlyweb",
		"tiddlywiki/filesystem",
		"tiddlywiki/markdown",
		"sycom/tiddlystatic"
	],
	"languages": [
		"fr-FR"
	],
	"themes": [
		"tiddlywiki/vanilla"
	],
	"build": {
		"dynamic": [
			"--rendertiddler","$:/plugins/tiddlywiki/tiddlyweb/save/offline","tiddlywiki.html","text/plain"],
		"feed": [
			"--rendertiddler","$:/plugins/sycom/atom-feed/atom.xml","atom.xml","text/plain"]
	},
	"config": {
		"retain-original-tiddler-path": true
	}
}
