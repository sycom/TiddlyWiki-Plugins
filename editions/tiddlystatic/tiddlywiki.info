{
   "description": "TiddlyStatic - an experiment to publish static website based upon TiddlyWiki and Node.js",
   "plugins": [
      "sycom/atom-feed",
      "sycom/tiddlystatic",
      "tiddlywiki/filesystem",
      "tiddlywiki/markdown",
      "tiddlywiki/tiddlyweb"
      ],
   "languages": [
      "fr-FR"
      ],
   "themes": [
      "tiddlywiki/vanilla"
      ],
   "build": {
      "images": [
         "--savetiddlers", "[is[image]has[_canonical_uri]]", "images",
         "--setfield", "[is[image]has[_canonical_uri]]", "_canonical_uri", "$:/core/templates/canonical-uri-external-image", "text/plain",
         "--setfield", "[tag[external-text]]", "_canonical_uri", "$:/core/templates/canonical-uri-external-text", "text/plain"
         ],
      "index": [
         "--rendertiddler", "$:/plugins/sycom/tiddlystatic/export/index.html", "index.html", "text/plain"
         ],
      "dynamic": [
         "--rendertiddler", "$:/plugins/tiddlywiki/tiddlyweb/save/offline", "TiddlyStatic.html", "text/plain"
         ],
      "wiki": [
         "--rendertiddlers", "[!is[system]!has[draft.of]] +[all[tiddlers]links[]missing[]]", "$:/plugins/sycom/tiddlystatic/Templates/wiki/tiddler.html", "wiki", "text/plain"
         ],
      "styles":[
         "--rendertiddler", "$:/plugins/sycom/tiddlystatic/export/symple.css", "styles/symple.css", "text/plain"
         ],
      "favicon": [
         "--savetiddler", "$:/favicon.ico", "favicon.ico"
         ],
      "feed": [
         "--rendertiddler", "$:/plugins/sycom/atom-feed/atom.xml", "atom.xml", "text/plain"
         ]
   },
   "config": {
      "retain-original-tiddler-path": true
      }
}
