{
	"description": "writer helps you write and publish",
	"plugins": [
			"tiddlywiki/tiddlyweb",
			"tiddlywiki/filesystem",
			"tiddlywiki/markdown"
	],
	"languages": [
	 "fr-FR"
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"index": [
			"--rendertiddler","$:/plugins/tiddlywiki/tiddlyweb/save/offline","../../output/writer.html","text/plain"]
	},
	"config": {
		"retain-original-tiddler-path": true
	}
}
